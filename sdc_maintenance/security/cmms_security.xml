<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record model="ir.module.category" id="module_category_maintenance">
      <field name="name">Maintenance</field>
      <field name="description">Maintenance</field>
      <field name="sequence">5</field>
  </record>

  <record id="group_maintenance_user" model="res.groups">
      <field name="name">User</field>
      <field name="category_id" ref="maintenance.module_category_maintenance"/>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
  </record>

  <record id="group_maintenance_manager" model="res.groups">
      <field name="name">Manager</field>
      <field name="category_id" ref="maintenance.module_category_maintenance"/>
      <field name="implied_ids" eval="[(4, ref('group_maintenance_user'))]"/>
  </record>
</odoo>
