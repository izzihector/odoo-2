<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.module.category" id="module_category_regulatory_management">
            <field name="name">regulatory Management</field>
            <field name="description">Helps you handle your regulatory needs</field>
            <field name="sequence">5</field>
        </record>

        <record id="group_regulatory_user" model="res.groups">
            <field name="name">Chair User</field>
            <field name="category_id" ref="regulatory_management.module_category_regulatory_management"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_regulatory_manager" model="res.groups">
            <field name="name">regulatory Manager</field>
            <field name="category_id" ref="regulatory_management.module_category_regulatory_management"/>
            <field name="implied_ids" eval="[(4, ref('group_regulatory_user'))]"/>
        </record>

    </data>
</odoo>
